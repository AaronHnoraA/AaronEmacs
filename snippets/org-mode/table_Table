\begin{table}
	\caption{$1}\label{tab:$2}
	\begin{center}
		\begin{tabular}[c]{l|l}
			\hline
			\multicolumn{1}{c|}{\textbf{$3}} & 
			\multicolumn{1}{c}{\textbf{$4}} \\\\
			\hline
			a & b \\\\
			c & d \\\\
			$5
			\hline
		\end{tabular}
	\end{center}
\end{table}
$0  `(when (looking-at "\n") (delete-char 1))`